    "A": "–í—ã–≥—Ä—É–∂–∞–µ—Ç —Å—Ç—Ä–∞–Ω–∏—Ü—É, –∫–æ—Ç–æ—Ä–∞—è —Ä–µ–∂–µ –≤—Å–µ–≥–æ –∑–∞–≥—Ä—É–∂–∞–ª–∞—Å—å –≤ –ø–∞–º—è—Ç—å.",
    "B": "–í—ã–≥—Ä—É–∂–∞–µ—Ç —Å—Ç—Ä–∞–Ω–∏—Ü—É —Å –Ω–∞–∏–º–µ–Ω—å—à–µ–π —á–∞—Å—Ç–æ—Ç–æ–π –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è.",
    "C": "–í—ã–≥—Ä—É–∂–∞–µ—Ç —Å–∞–º—É—é –±–æ–ª—å—à—É—é –ø–æ —Ä–∞–∑–º–µ—Ä—É —Å—Ç—Ä–∞–Ω–∏—Ü—É.",
    "D": "–í—ã–≥—Ä—É–∂–∞–µ—Ç —Å—Ç—Ä–∞–Ω–∏—Ü—É, —Å–æ–∑–¥–∞—é—â—É—é –Ω–∞–∏–±–æ–ª—å—à—É—é –Ω–∞–≥—Ä—É–∑–∫—É –Ω–∞ –∫—ç—à.",
    "–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç": "B"
  }
];

        // ==================================================================================
        // ‚Üë‚Üë‚Üë‚Üë‚Üë –ö–û–ù–ï–¶ –ó–û–ù–´ –í–°–¢–ê–í–ö–ò JSON ‚Üë‚Üë‚Üë‚Üë‚Üë
        // ==================================================================================


        let allQuestions = [];
        let currentQuestions = [];
        let currentQuestionIndex = 0;
        let userAnswers = {};
        
        // --- DOM Elements ---
        const startQuizBtn = document.getElementById('startQuiz');
        const rangeToggle = document.getElementById('rangeToggle');
        const rangeSelectorGroup = document.getElementById('rangeSelectorGroup');
        const questionCountGroup = document.getElementById('questionCountGroup');
        const questionOrderGroup = document.getElementById('questionOrderGroup');
        const statusMessage = document.getElementById('statusMessage');

        // --- Initialization ---
        window.addEventListener('DOMContentLoaded', () => {
            if (questionsData && questionsData.length > 0) {
                allQuestions = questionsData;
                initSetup();
            } else {
                statusMessage.style.backgroundColor = 'rgba(231, 76, 60, 0.3)';
                statusMessage.style.borderColor = '#e74c3c';
                statusMessage.textContent = '–û—à–∏–±–∫–∞: –°–ø–∏—Å–æ–∫ –≤–æ–ø—Ä–æ—Å–æ–≤ –ø—É—Å—Ç. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∫–æ–¥ –∏ –≤—Å—Ç–∞–≤—å—Ç–µ JSON.';
            }
        });

        function initSetup() {
            document.getElementById('questionCount').max = allQuestions.length;
            document.getElementById('questionCount').value = Math.min(10, allQuestions.length);
            document.getElementById('rangeStart').max = allQuestions.length;
            document.getElementById('rangeEnd').max = allQuestions.length;
            document.getElementById('rangeEnd').value = allQuestions.length;
            
            // –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫ –¥–∏–∞–ø–∞–∑–æ–Ω–æ–≤
            generateRangePresets();

            startQuizBtn.disabled = false;
            statusMessage.textContent = `‚úÖ –£—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω–æ –≤–æ–ø—Ä–æ—Å–æ–≤: ${allQuestions.length}`;
        }

        // --- –§—É–Ω–∫—Ü–∏—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ –∫–Ω–æ–ø–æ–∫ —à–∞–±–ª–æ–Ω–æ–≤ ---
        function generateRangePresets() {
            const container = document.getElementById('rangePresets');
            container.innerHTML = ''; // –û—á–∏—Å—Ç–∫–∞
            const total = allQuestions.length;
            const step = 50; // –®–∞–≥ –≤–æ–ø—Ä–æ—Å–æ–≤

            if (total <= 0) return;

            // –ö–Ω–æ–ø–∫–∞ "–í—Å–µ –≤–æ–ø—Ä–æ—Å—ã" –ø–µ—Ä–≤–∞—è
            const allBtn = document.createElement('button');
            allBtn.type = 'button';
            allBtn.className = 'preset-btn';
            allBtn.textContent = `–í—Å–µ (${1}-${total})`;
            allBtn.style.background = 'rgba(39, 174, 96, 0.4)';
            allBtn.style.borderColor = '#27ae60';
            allBtn.addEventListener('click', () => {
                document.getElementById('rangeStart').value = 1;
                document.getElementById('rangeEnd').value = total;
            });
            container.appendChild(allBtn);

            // –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫ –ø–æ —à–∞–≥–∞–º (1-50, 51-100 –∏ —Ç.–¥.)
            for (let i = 1; i <= total; i += step) {
                const end = Math.min(i + step - 1, total);
                const btn = document.createElement('button');
                btn.type = 'button';
                btn.className = 'preset-btn';
                btn.textContent = `${i}-${end}`;
                
                btn.addEventListener('click', () => {
                    document.getElementById('rangeStart').value = i;
                    document.getElementById('rangeEnd').value = end;
                    
                    // –≠—Ñ—Ñ–µ–∫—Ç –Ω–∞–∂–∞—Ç–∏—è –Ω–∞ –∏–Ω–ø—É—Ç—ã
                    const inputs = document.querySelectorAll('.range-inputs input');
                    inputs.forEach(input => {
                        input.style.borderColor = '#4ca1af';
                        input.style.backgroundColor = 'rgba(76, 161, 175, 0.3)';
                        setTimeout(() => {
                            input.style.borderColor = '';
                            input.style.backgroundColor = '';
                        }, 300);
                    });
                });
                
                container.appendChild(btn);
            }
        }
        
        // --- Event Listeners ---
        rangeToggle.addEventListener('change', function() {
            const useRange = this.checked;
            rangeSelectorGroup.classList.toggle('hidden', !useRange);
            questionCountGroup.classList.toggle('disabled', useRange);
            questionOrderGroup.classList.toggle('disabled', useRange);
        });

        startQuizBtn.addEventListener('click', function() {
            if (allQuestions.length === 0) return;
            
            const useRange = rangeToggle.checked;
            const answerOrder = document.getElementById('answerOrder').value;

            if (useRange) {
                let start = parseInt(document.getElementById('rangeStart').value) || 1;
                let end = parseInt(document.getElementById('rangeEnd').value) || allQuestions.length;
                
                start = Math.max(1, start);
                end = Math.min(allQuestions.length, end);

                if (start > end) [start, end] = [end, start];
                
                document.getElementById('rangeStart').value = start;
                document.getElementById('rangeEnd').value = end;
                
                currentQuestions = allQuestions.slice(start - 1, end);
            } else {
                const questionCount = parseInt(document.getElementById('questionCount').value);
                const questionOrder = document.getElementById('questionOrder').value;
                
                let questions = [...allQuestions];
                if (questionOrder === 'random') {
                    questions = shuffleArray(questions);
                }
                currentQuestions = questions.slice(0, questionCount);
            }
            
            if (currentQuestions.length === 0) {
                alert("–ù–µ –Ω–∞–π–¥–µ–Ω–æ –≤–æ–ø—Ä–æ—Å–æ–≤ –¥–ª—è –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö –∫—Ä–∏—Ç–µ—Ä–∏–µ–≤.");
                return;
            }

            if (answerOrder === 'random') {
                currentQuestions = currentQuestions.map(q => shuffleAnswers(q));
            }

            currentQuestionIndex = 0;
            userAnswers = {};
            
            document.getElementById('setupSection').style.display = 'none';
            document.getElementById('quizSection').style.display = 'block';

            initializeQuiz();
        });
        
        document.getElementById('restartBtn').addEventListener('click', () => {
            document.getElementById('resultsSection').style.display = 'none';
            document.getElementById('setupSection').style.display = 'block';
            document.getElementById('reviewSection').style.display = 'none';
            
            // –°–±—Ä–æ—Å –Ω–∞—Å—Ç—Ä–æ–µ–∫
            document.getElementById('questionCount').value = Math.min(10, allQuestions.length);
            document.getElementById('questionOrder').value = 'original';
            document.getElementById('answerOrder').value = 'original';
            rangeToggle.checked = false;
            rangeSelectorGroup.classList.add('hidden');
            questionCountGroup.classList.remove('disabled');
            questionOrderGroup.classList.remove('disabled');
        });

        // --- Core Quiz Functions ---
        function shuffleArray(array) {
            const shuffled = [...array];
            for (let i = shuffled.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [shuffled[i], shuffled[j]] = [shuffled[j], shuffled[i]];
            }
            return shuffled;
        }

        function shuffleAnswers(question) {
            const answers = [{ key: 'A', value: question.A }, { key: 'B', value: question.B }, { key: 'C', value: question.C }, { key: 'D', value: question.D }];
            const shuffled = shuffleArray(answers.filter(a => a.value)); // –§–∏–ª—å—Ç—Ä—É–µ–º –ø—É—Å—Ç—ã–µ –≤–∞—Ä–∏–∞–Ω—Ç—ã
            const newQuestion = { ...question };
            const correctOriginalKey = question['–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç'];
            
            shuffled.forEach((answer, index) => {
                const newKey = ['A', 'B', 'C', 'D'][index];
                newQuestion[newKey] = answer.value;
                if(answer.key === correctOriginalKey) {
                    newQuestion['–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç'] = newKey;
                }
            });

            // –û–±–Ω—É–ª—è–µ–º –Ω–µ–∏—Å–ø–æ–ª—å–∑—É–µ–º—ã–µ –ø–æ–ª—è –≤–∞—Ä–∏–∞–Ω—Ç–æ–≤
            for(let i = shuffled.length; i < 4; i++) {
                const newKey = ['A', 'B', 'C', 'D'][i];
                newQuestion[newKey] = null;
            }
            return newQuestion;
        }


        function initializeQuiz() {
            createNavigation();
            displayQuestion();
            updateProgress();
        }

        function createNavigation() {
            const navButtons = document.getElementById('navButtons');
            navButtons.innerHTML = '';
            for (let i = 0; i < currentQuestions.length; i++) {
                const btn = document.createElement('button');
                btn.className = 'nav-btn';
                btn.textContent = i + 1;
                btn.addEventListener('click', () => goToQuestion(i));
                navButtons.appendChild(btn);
            }
        }

        function displayQuestion() {
            const question = currentQuestions[currentQuestionIndex];
            document.getElementById('questionText').innerHTML = question['–í–æ–ø—Ä–æ—Å'];

            const optionsContainer = document.getElementById('optionsContainer');
            optionsContainer.innerHTML = '';
            ['A', 'B', 'C', 'D'].forEach(optionKey => {
                if (!question[optionKey]) return; // –ù–µ —Å–æ–∑–¥–∞–≤–∞—Ç—å –±–ª–æ–∫ –¥–ª—è –ø—É—Å—Ç–æ–≥–æ –≤–∞—Ä–∏–∞–Ω—Ç–∞
                const optionDiv = document.createElement('div');
                optionDiv.className = 'option';
                optionDiv.innerHTML = `<strong>${optionKey}:</strong> ${question[optionKey]}`;
                optionDiv.addEventListener('click', () => {
                    // –ó–∞–ø—Ä–µ—Ç–∏—Ç—å –ø–æ–≤—Ç–æ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç
                    if (userAnswers[currentQuestionIndex] === undefined) {
                        selectAnswer(optionKey);
                    }
                });
                optionsContainer.appendChild(optionDiv);
            });
            
            // –ü—Ä–∏–º–µ–Ω–∏—Ç—å —Å—Ç–∏–ª–∏, –µ—Å–ª–∏ –Ω–∞ –≤–æ–ø—Ä–æ—Å —É–∂–µ –æ—Ç–≤–µ—Ç–∏–ª–∏
            if (userAnswers[currentQuestionIndex] !== undefined) {
                applyAnswerStyles();
            }

            updateNavigation();
            updateButtons();
        }
        
        function selectAnswer(option) {
            userAnswers[currentQuestionIndex] = option;
            applyAnswerStyles();
            updateNavigation();
            updateProgress();
        }

        function applyAnswerStyles() {
            const userAnswer = userAnswers[currentQuestionIndex];
            if (userAnswer === undefined) return;

            const correctAnswer = currentQuestions[currentQuestionIndex]['–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç'];
            const options = document.querySelectorAll('.option');
            
            let userOptionElement = null;
            let correctOptionElement = null;

            options.forEach(opt => {
                const optionLetter = opt.textContent.trim().substring(0, 1);
                if (optionLetter === userAnswer) {
                    userOptionElement = opt;
                }
                if (optionLetter === correctAnswer) {
                    correctOptionElement = opt;
                }
            });

            // –°—Ç–∏–ª–∏–∑—É–µ–º –≤—ã–±–æ—Ä –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
            if (userOptionElement) {
                userOptionElement.classList.add('selected');
                if (userAnswer === correctAnswer) {
                    userOptionElement.classList.add('correct');
                } else {
                    userOptionElement.classList.add('incorrect');
                }
            }
            
            // –í—Å–µ–≥–¥–∞ –ø–æ–¥—Å–≤–µ—á–∏–≤–∞–µ–º –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç, –µ—Å–ª–∏ –æ—Ç–≤–µ—Ç –±—ã–ª –¥–∞–Ω
            if (correctOptionElement) {
                 correctOptionElement.classList.add('correct');
            }
        }

        function goToQuestion(index) {
            currentQuestionIndex = index;
            displayQuestion();
        }

        function updateNavigation() {
            document.querySelectorAll('.nav-btn').forEach((btn, index) => {
                btn.classList.remove('current', 'answered');
                if (index === currentQuestionIndex) {
                    btn.classList.add('current');
                    btn.scrollIntoView({ behavior: 'smooth', block: 'nearest', inline: 'center' });
                }
                if (userAnswers[index] !== undefined) {
                    btn.classList.add('answered');
                }
            });
        }

        function updateProgress() {
            const answered = Object.keys(userAnswers).length;
            const progress = currentQuestions.length > 0 ? (answered / currentQuestions.length) * 100 : 0;
            document.getElementById('progressFill').style.width = `${progress}%`;
        }

        function updateButtons() {
            document.getElementById('prevBtn').style.display = currentQuestionIndex === 0 ? 'none' : 'inline-block';
            const isLastQuestion = currentQuestionIndex === currentQuestions.length - 1;
            document.getElementById('nextBtn').style.display = isLastQuestion ? 'none' : 'inline-block';
            document.getElementById('finishBtn').style.display = 'inline-block'; // –í—Å–µ–≥–¥–∞ –ø–æ–∫–∞–∑—ã–≤–∞—Ç—å –∫–Ω–æ–ø–∫—É –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è
        }

        document.getElementById('prevBtn').addEventListener('click', () => { if (currentQuestionIndex > 0) goToQuestion(currentQuestionIndex - 1) });
        document.getElementById('nextBtn').addEventListener('click', () => { if (currentQuestionIndex < currentQuestions.length - 1) goToQuestion(currentQuestionIndex + 1) });
        document.getElementById('finishBtn').addEventListener('click', showResults);
        
        document.getElementById('showMistakesBtn').addEventListener('click', function() {
            const reviewSection = document.getElementById('reviewSection');
            if (reviewSection.style.display === 'none') {
                const mistakesContainer = document.getElementById('mistakesContainer');
                mistakesContainer.innerHTML = '';
                window.currentMistakes.forEach((mistake, index) => {
                    const userAnswerText = mistake.options[mistake.userAnswer] || '–ù–µ—Ç –æ—Ç–≤–µ—Ç–∞';
                    const correctAnswerText = mistake.options[mistake.correctAnswer] || '–ù–µ—Ç –æ—Ç–≤–µ—Ç–∞';
                    mistakesContainer.innerHTML += `
                        <div class="mistake-item">
                            <div class="mistake-question">${index + 1}. ${mistake.question}</div>
                            <div class="mistake-answers">
                                <div><strong>–í–∞—à –æ—Ç–≤–µ—Ç:</strong> <span class="incorrect">${mistake.userAnswer}: ${userAnswerText}</span></div>
                                <div><strong>–í–µ—Ä–Ω—ã–π –æ—Ç–≤–µ—Ç:</strong> <span class="correct">${mistake.correctAnswer}: ${correctAnswerText}</span></div>
                            </div>
                        </div>`;
                });
                reviewSection.style.display = 'block';
                this.textContent = '–°–∫—Ä—ã—Ç—å –æ—à–∏–±–∫–∏';
            } else {
                reviewSection.style.display = 'none';
                this.textContent = `–ü–æ–∫–∞–∑–∞—Ç—å ${window.currentMistakes.length} –æ—à–∏–±${window.currentMistakes.length === 1 ? '–∫—É' : (window.currentMistakes.length > 4 ? '–æ–∫' : '–∫–∏')}`;
            }
        });

        function showResults() {
            let correct = 0;
            const mistakes = [];
            currentQuestions.forEach((question, index) => {
                if (userAnswers[index] === question['–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç']) {
                    correct++;
                } else if (userAnswers[index] !== undefined) {
                    mistakes.push({
                        question: question['–í–æ–ø—Ä–æ—Å'],
                        userAnswer: userAnswers[index],
                        correctAnswer: question['–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç'],
                        options: { A: question.A, B: question.B, C: question.C, D: question.D }
                    });
                }
            });

            const total = currentQuestions.length;
            const percentage = total > 0 ? Math.round((correct / total) * 100) : 0;

            document.getElementById('scoreDisplay').textContent = `${percentage}%`;
            document.getElementById('totalQuestions').textContent = total;
            document.getElementById('correctAnswers').textContent = correct;
            document.getElementById('incorrectAnswers').textContent = total - correct;
            document.getElementById('finalScore').textContent = `${percentage}%`;

            window.currentMistakes = mistakes;
            const showMistakesBtn = document.getElementById('showMistakesBtn');
            if (mistakes.length > 0) {
                showMistakesBtn.style.display = 'inline-block';
                const mistakeCount = mistakes.length;
                const mistakeWord = mistakeCount === 1 ? '–æ—à–∏–±–∫—É' : (mistakeCount > 4 ? '–æ—à–∏–±–æ–∫' : '–æ—à–∏–±–∫–∏');
                showMistakesBtn.textContent = `–ü–æ–∫–∞–∑–∞—Ç—å ${mistakeCount} ${mistakeWord}`;
            } else {
                showMistakesBtn.style.display = 'none';
            }

            document.getElementById('quizSection').style.display = 'none';
            document.getElementById('resultsSection').style.display = 'block';
        }
    </script>
</body>
</html>
